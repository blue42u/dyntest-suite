ifeq ($(HPCTOOLKIT_PREFIX),)
error HPCTOOLKIT_PREFIX needs to be defined!
endif

ifeq ($(HPCTOOLKIT_REAL_DIR),)
error HPCToolkit/use.tup should have been included before this!
endif

EX_DEPS += $(HPCTOOLKIT_EX_DEP)
CFG_FLAGS += $(HPCTOOLKIT_PREFIX)$(HPCTOOLKIT_DUMMY_DIR)
TRANSFORMS += $(HPCTOOLKIT_DUMMY_DIR)=$(HPCTOOLKIT_REAL_DIR)
