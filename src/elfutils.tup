# Include this file wherever you want Elfutils built.
# Installation prefix is under ./install.

preload elfutils elfutils/m4 elfutils/backends elfutils/config elfutils/lib
preload elfutils/libasm elfutils/libcpu elfutils/libdw elfutils/libdwelf
preload elfutils/libdwfl elfutils/libebl elfutils/libelf elfutils/src
preload elfutils/tests elfutils/po elfutils/lib/valgrind
run $(TUP_CWD)/../build/automake.sh src/elfutils $(TUP_CWD)/elfutils install/ \
  elfutils '$(EX_DEPS)' "$(TRANSFORMS)" --enable-maintainer-mode
